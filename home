<script>
     var lineDrawing = function () {
      /*DEMO*/
      anime({
        targets: ".line-drawing-demo .lines path",
        strokeDashoffset: [anime.setDashoffset, 0],
        easing: "easeInOutSine",
        duration: 5000,
        delay: function (el, i) {
          return i * 250;
        },
        direction: "alternate",
        loop: true,
      });
      /*DEMO*/
    };
    lineDrawing();

</script>
